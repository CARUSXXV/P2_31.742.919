<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Contactos Registrados</title>
    <link rel="stylesheet" href="/css/admin.css">
</head>
<body>
    <div class="container">
        <h2 class="title">Contactos registrados</h2>
        <div class="table-container">
            <table class="table">
                <thead class="table-header">
                    <tr>
                        <th>ID</th>
                        <th>Nombre</th>
                        <th>Email</th>
                        <th>Comentario</th>
                        <th>IP</th>
                        <th>Fecha</th>
                    </tr>
                </thead>
                <tbody class="table-body">
                    <% contacts.forEach(function(contact) { %>
                        <tr>
                            <td><%= contact.id %></td>
                            <td><%= contact.name %></td>
                            <td><%= contact.email %></td>
                            <td><%= contact.comment %></td>
                            <td><%= contact.ip_address %></td>
                            <td><%= contact.created_at %></td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</body>
</html>